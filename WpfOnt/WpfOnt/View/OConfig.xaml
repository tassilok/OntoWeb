<Window x:Class="WpfOnt.View.OConfig"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:viewModel="clr-namespace:WpfOnt.ViewModel"
        Title="{Binding WindowTitle}" Height="787.983" Width="600">
    <Window.DataContext>
        <viewModel:OConfigModel/>
    </Window.DataContext>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="722*"/>
            <RowDefinition Height="35*"/>
        </Grid.RowDefinitions>
        <ToolBar HorizontalAlignment="Left" Margin="0,1,0,0" VerticalAlignment="Top" Height="34" Width="592" Grid.Row="1">
            <Button Content="{Binding TextApply}"/>
            <Button Content="{Binding CancelApply}"/>
        </ToolBar>

    </Grid>
</Window>
